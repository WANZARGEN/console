{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": ["public/**", "src/**", "package.json", "tsconfig.json",
        "vite.config.ts", "postcss.config.js", "tailwind.config.js"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}