{
  "name": "mirinae-foundation",
  "version": "1.12.0-dev11",
  "description": "Mirinae(Cloudforet Design System) Foundation",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && npm run css",
    "dev": "rimraf dist && tsc-watch --project tsconfig.build.json --outDir ./dist --onSuccess \"tsc-alias -w -p tsconfig.build.json\"",
    "lint": "eslint --ext .js,.ts src",
    "format": "eslint --fix --ext .js,.ts src",
    "test": "vitest run",
    "test:watch": "vitest watch --ui",
    "css": "node scripts/css.js"
  },
  "files": [
    "/dist/*",
    "/src/*",
    "/css/*",
    "/tailwind.config.cjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudforet-io/console.git"
  },
  "author": "Cloudforet",
  "license": "Apache-2.0",
  "dependencies": {
    "lodash": "^4.17.21",
    "mirinae-foundation": "^1.12.0-dev11"
  },
  "devDependencies": {
    "@cloudforet/stylelint-config-custom": "*",
    "@types/node": "^16.11.43",
    "@vue/test-utils": "^1.3.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint-config-custom": "*",
    "postcss-config-custom": "*",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.6.11",
    "tsc-watch": "^6.0.0",
    "tsconfig": "*",
    "vite": "^4.3.9",
    "vite-plugin-stylelint": "^3.3.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./dist/*"
      ]
    }
  }
}
